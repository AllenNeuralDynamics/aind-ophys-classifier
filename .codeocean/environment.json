		"pip": {
			"packages": [
				{
					"name": "aind-data-schema",
					"version": "1.2.0"
				},
				{
					"name": "aind-qcportal-schema"
				},
				{
					"name": "h5py",
					"version": "3.12.1"
				},
				{
					"name": "pint",
					"version": "0.24.4"
				},
				{
					"name": "roicat[all]",
					"version": "1.4.4"
				},
				{
					"name": "sparse",
					"version": "0.15.4"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	},
	"env_variables": [
		{
			"name": "VERSION",
			"value": "0.1.0"
		},
		{
			"name": "CODE_URL",
